{
  "name": "inzo-portalempleado-calendar-lib",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --configuration=production",
    "build:lib": "ng build inzo-portalempleado-calendar-lib",
    "pack": "cd dist/inzo-portalempleado-calendar-lib && npm pack",
    "package": "npm run build:lib && npm run pack",
    "copy-license": "copy .LICENSE .dist/inzo-portalempleado-calendar-lib",
    "copy-readme": "copy .README.md .dist/inzo-portalempleado-calendar-lib",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "test-app": "ng test inzo-portalempleado-calendar-lib",
    "test-app-headless": "ng test inzo-portalempleado-calendar-lib --watch=false --browsers=ChromeHeadless",
    "test-lib": "ng test inzo-portalempleado-calendar-lib",
    "test-lib-headless": "ng test inzo-portalempleado-calendar-lib --watch=false --browsers=ChromeHeadless",
    "test-all": "npm run test-app && test-lib",
    "test-all-headless": "npm run test-app-headless && npm run test-lib-headless",
    "lint": "ng lint",
    "e2e": "ng e2e --protractor-config=e2e/protractor.conf",
    "e2e-headless": "ng e2e --protractor-config=e2e/protractor.headless.conf",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@schematics/angular": "^12.1.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.0.0",
    "rxjs": "^7.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1201.1",
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular-devkit/build-optimizer": "^0.1201.1",
    "@angular-devkit/build-webpack": "^0.1201.1",
    "@angular-devkit/core": "^12.1.1",
    "@angular-devkit/schematics": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@angular/language-service": "^12.1.1",
    "@ngtools/webpack": "^12.1.1",
    "@types/jasmine": "^3.7.7",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^16.0.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^12.1.0",
    "protractor": "^7.0.0",
    "ts-node": "^10.0.0",
    "tsickle": "^0.43.0",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
